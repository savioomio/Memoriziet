{
  "expo": {
    "newArchEnabled": true,
    "name": "Memoriziet",
    "slug": "Memoriziet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icone.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo.jpg"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icone.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}